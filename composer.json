        "doctrine/dbal": "2.3.0",
        "ymmtmsys/command": "0.2.0",
        "jdorn/sql-formatter": "v1.2.17"
    },
    "require-dev": {
        "mikey179/vfsStream": "v1.2.0",
        "mockery/mockery": "0.9.0",
        "phpunit/phpunit": "4.0.15"
