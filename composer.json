{
    "name": "jamayka/db-patcher",
    "description": "DB patcher",
    "license": "MIT",
    "authors": [
        {
            "name": "Alexey Khokhryakov",
            "email": "alexey.khokhryakov@gmail.com"
        }
    ],
    "bin": ["bin/db-patcher.php"],
    "require": {
        "php": ">=5.3.3",
        "fusepump/cli.php": "0.5.*",
        "doctrine/dbal": "2.3.*",
        "symfony/process": "2.4.*",
        "jdorn/sql-formatter": "v1.2.17"
    },
    "require-dev": {
        "mikey179/vfsStream": "v1.2.0",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "3.7.*"
    },
    "autoload": {
        "psr-4": {
            "DBPatcher\\": "src/"
        },
        "files": [
           "src/functions.php",
           "src/Storage/functions.php",
           "src/Strategy/functions.php",
           "src/Apply/functions.php",
           "src/Cli/functions.php"
        ]
    }
}

